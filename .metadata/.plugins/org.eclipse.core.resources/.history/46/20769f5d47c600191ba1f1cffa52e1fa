package PagaLayer;

import java.util.concurrent.TimeUnit;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.chrome.ChromeOptions;

public class LoginPage {

	public static void main(String[] args)
		{
		 String exe = "chromedriver.exe";
			final String webSite = "http://www.naukri.com";
			final String binaryPath = "C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe";
			String path = "C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe";
			//System.setProperty("webdriver.chrome.driver", path);
			ChromeOptions chromeOpt= new ChromeOptions();
			chromeOpt.setBinary(binaryPath);
			
			WebDriver driver = new ChromeDriver (chromeOpt);
			driver.get(webSite);
			driver.manage ().timeouts ().implicitlyWait (10, TimeUnit.SECONDS);
			driver.manage().window().maximize();

		
		}
}
